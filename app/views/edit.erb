<form action='/posts/<%= @post.id %>' method="post">
  <input id="hidden" type="hidden" name="_method" value="patch">
  <h3>Post Title:</h3>
    <input id="name" input type="text" name="post[name]" value=<%= @post.name %>>
  <h3>Post:</h3>
  <input id="content" input type="text" name="post[content]" value=<%= @post.content %>>
  <br>
  <input id="submit" type="submit" name="submit" value="submit">
</form>
